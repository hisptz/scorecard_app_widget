{
    "plugins": ["react"],
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true,
        },
        "ecmaVersion": 2018,
        "sourceType": "module",
      },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "react/jsx-props-no-spreading": "off"
    }
}